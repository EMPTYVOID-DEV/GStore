{
  "name": "@gstore-org/cli",
  "version": "1.0.0",
  "description": "This is the GStore cli used for automated tasks.",
  "type": "module",
  "scripts": {
    "dev": "bun run ./src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "start": "bun run ./dist/index.js",
    "lint": "eslint ."
  },
  "license": "MIT",
  "author": "Aymen Keskas",
  "maintainers": [
    "Aymen keskas <keskasaymen8@gmail.com> (https://empty-void.vercel.app/)"
  ],
  "keywords": [
    "GStore",
    "Cli",
    "workflow",
    "backup",
    "automation"
  ],
  "main": "dist/index.js",
  "bin": {
    "gstore-cli": "./dist/index.js"
  },
  "files": [
    "./dist/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "bun-types": "^1.1.34",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  }
}
