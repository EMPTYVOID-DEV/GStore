{
  "name": "@gstore-org/api",
  "version": "1.2.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch run src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --external sharp --external hono-rate-limiter --sourcemap",
    "start": "pnpm run push-schema && bun run ./dist/index.js",
    "db-studio": "pnpm drizzle-kit studio",
    "migrate": "pnpm drizzle-kit generate",
    "push-migrations": "pnpm drizzle-kit migrate",
    "push-schema": "pnpm drizzle-kit push",
    "lint": "eslint ."
  },
  "dependencies": {
    "@hono/zod-openapi": "^0.16.0",
    "@scalar/hono-api-reference": "^0.5.149",
    "archiver": "^7.0.1",
    "drizzle-orm": "0.36.0",
    "drizzle-zod": "^0.5.1",
    "fluent-ffmpeg": "^2.1.3",
    "fp-ts": "^2.16.9",
    "hono": "^4.5.11",
    "hono-rate-limiter": "^0.4.0",
    "nanoid": "^5.0.7",
    "pdf-lib": "^1.17.1",
    "pg": "^8.12.0",
    "sharp": "^0.33.5",
    "tmp": "^0.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/archiver": "^6.0.2",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/pg": "^8.11.8",
    "@types/tmp": "^0.2.6",
    "bun-types": "^1.1.34",
    "drizzle-kit": "^0.27.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.12.2"
  }
}
